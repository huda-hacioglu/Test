name: PROJECT

conda_env: conda_env.yaml

entry_points:
  main:
    parameters:
      learning_rate: {type: float, default: 0.1}
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 2}
      random_state: {type: int, default: 35}
      real_testssize: {type: float, default: 0.4}
    command: "python main.sh {learning_rate} {n_estimators} {max_depth} {random_state} {real_testssize}"